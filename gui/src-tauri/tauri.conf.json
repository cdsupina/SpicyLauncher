{
  "package": {
    "productName": "fish-launcher",
    "version": "0.1.0"
  },
  "build": {
    "distDir": "../public",
    "devPath": "http://localhost:8080",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "tauri": {
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.fishfight.launcher",
      "icon": ["icons/32x32.png", "icons/128x128.png", "icons/icon.ico"],
      "resources": [],
      "externalBin": [],
      "copyright": "Copyright (c) Orhun ParmaksÄ±z 2022. All rights reserved.",
      "category": "Game",
      "shortDescription": "Launcher for Fish Fight",
      "longDescription": "A cross-platform launcher for playing Fish Fight",
      "deb": {
        "depends": [],
        "useBootstrapper": false
      },
      "macOS": {
        "frameworks": [],
        "minimumSystemVersion": "",
        "useBootstrapper": false,
        "exceptionDomain": "",
        "signingIdentity": null,
        "entitlements": null
      },
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "updater": {
      "active": false
    },
    "allowlist": {
      "all": true
    },
    "windows": [
      {
        "title": "Fish Fight Launcher",
        "width": 955,
        "height": 600,
        "resizable": false,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": "default-src blob: data: filesystem: ws: wss: http: https: tauri: 'unsafe-eval' 'unsafe-inline' 'self' img-src: 'self'"
    }
  }
}
